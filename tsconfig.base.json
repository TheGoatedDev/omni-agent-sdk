{
	"compilerOptions": {
		"target": "ES2022",
		"lib": ["ES2022", "esnext.disposable", "esnext.asynciterable", "dom"],
		"module": "Node16",
		"moduleResolution": "Node16",
		"strict": true,
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"verbatimModuleSyntax": true,
		"isolatedModules": true,
		"noUncheckedIndexedAccess": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true
	}
}
